# Service Configuration
NODE_ENV=development
APP_PORT=3000

# Azure Service Bus
AZURE_SERVICE_BUS_CONNECTION_STRING=Endpoint=sb://example.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=examplekey
AZURE_SERVICE_BUS_INVOICE_TOPIC=invoice-events
AZURE_SERVICE_BUS_OCR_SUBSCRIPTION=ocr-worker

# Azure AI Document Intelligence
AZURE_DOC_INTELLIGENCE_ENDPOINT=https://example.cognitiveservices.azure.com/
AZURE_DOC_INTELLIGENCE_KEY=your_cognitive_service_key

# Internal Services
PRODUCT_SERVICE_INTERNAL_URL=http://product-service:3000

# Scheduler Configuration
# Cron Format: Second Minute Hour Day-of-Month Month Day-of-Week
# Default: Every hour
CRON_SCHEDULE_TRANSFER_EXPIRY=0 0 * * * *